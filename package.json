{
    "name": "funiciz",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "lint": "eslint .",
        "start": "react-native start",
        "test": "jest",
        "android:dev": "ENVFILE=.env react-native run-android",
        "android:staging": "ENVFILE=.env.staging react-native run-android",
        "android:prod": "ENVFILE=.env.prod react-native run-android",
        "build-android:dev": "cd ./android && ./gradlew clean && ./gradlew assembleRelease",
        "build-android:staging": "cd ./android && ./gradlew clean && cross-env ENVFILE=.env.staging ./gradlew assembleRelease",
        "build-android:production": "cd ./android && ./gradlew clean && cross-env ENVFILE=.env.prod ./gradlew assembleRelease",
        "generate-dotenv": "sh util/generate-dotenv.sh > .env",
        "device:adb": "adb reverse tcp:8081 tcp:8081",
        "clean": "yarn cache clean && cd android && ./gradlew clean && cd ..",
        "aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
        "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd .."
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.17.12",
        "@react-navigation/bottom-tabs": "^6.5.7",
        "@react-navigation/native": "^6.1.6",
        "@react-navigation/native-stack": "^6.9.12",
        "babel-plugin-module-resolver": "^5.0.0",
        "react": "18.2.0",
        "react-native": "0.72.1",
        "react-native-google-mobile-ads": "^11.1.1",
        "react-native-iap": "12.10.5",
        "react-native-safe-area-context": "^4.5.0",
        "react-native-screens": "^3.20.0",
        "react-native-simple-toast": "^2.0.0",
        "react-native-sound": "^0.11.2",
        "react-native-sound-player": "^0.13.2",
        "react-redux": "^8.0.5",
        "redux": "^4.2.1",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^2.4.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.0",
        "@react-native/eslint-config": "^0.72.2",
        "@react-native/metro-config": "^0.72.7",
        "@tsconfig/react-native": "^3.0.0",
        "@types/metro-config": "^0.76.3",
        "@types/react": "^18.0.24",
        "@types/react-test-renderer": "^18.0.0",
        "babel-jest": "^29.2.1",
        "eslint": "^8.19.0",
        "jest": "^29.2.1",
        "metro-react-native-babel-preset": "0.76.5",
        "prettier": "^2.4.1",
        "react-test-renderer": "18.2.0",
        "typescript": "4.8.4"
    },
    "engines": {
        "node": ">=16"
    }
